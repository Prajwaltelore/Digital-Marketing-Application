            "currency": "INR",
            "name": "Albos Technologies",
            "description": orderdata.description,
            "order_id": orderdata.order_id,
            "handler": function (response) {
                $.ajax({
                    url: "/payment/" + response.razorpay_payment_id,
                    method: "post",
                    dataType: "json",
                    headers: { "Authorization": 'Bearer' + localStorage.getItem('auth') },
                    data: { order_id: response.razorpay_order_id, user: user },
                    success: function (data) {
                        if (data.status) {
                            subscription(data.data);
                            window.location.href = 'success';
                        } else {
                            window.location.href = 'fail';
                        }
                    }
                });
            },
            "prefill": {
                "name": user.firstname + " " + user.lastname,
                "email": user.email,
                "contact": user.mobile
            },
            "theme": {
                "color": "#3399cc"
            }
        };
        var rzp1 = new Razorpay(options);
        rzp1.on('payment.failed', function (response) {
            alert(response.error.code);
            alert(response.error.description);
            alert(response.error.source);
            alert(response.error.step);
            alert(response.error.reason);
            alert(response.error.metadata.order_id);
            alert(response.error.metadata.payment_id);
        });

        rzp1.open();
    }

    function createOrder(packageid) {

        let auth = JSON.parse(localStorage.getItem('auth'));
        if(auth == null){
            alert("Please login to proceed for payment");
        }
        else{
            let user = auth.userData;
        $.ajax({
            url: "/orders/" + packageid,
            method: "post",
            dataType: "json",
            headers: { "Authorization": 'Bearer' + localStorage.getItem('auth') },
            data: user,
            success: function (response) {
                if (response.status) {
                    getway(response.data);
                } else {
                    window.location.href = 'login';
                }
            },
            error: function (error) {
                console.log(error);
            }
        });
        }
    }

